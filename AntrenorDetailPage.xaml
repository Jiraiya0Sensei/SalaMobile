<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SalaMobile.AntrenorDetailPage"
             Title="AntrenorDetailPage">
    <StackLayout Margin="20">
        <Label Text="Nume: " FontAttributes="Bold" />
        <Label Text="{Binding Nume}" />
        
        <Label Text="Telefon: " FontAttributes="Bold" />
        <Label Text="{Binding Telefon}" />
               
        <Label Text="Email: " FontAttributes="Bold" />
        <Label Text="{Binding Email}" />
            <Button Text="Edit" Clicked="OnEditButtonClicked" />
            <Button Text="Delete" Clicked="OnDeleteButtonClicked" />
    </StackLayout>
</ContentPage>