<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SalaMobile.SportDetailPage"
             Title="SportDetailPage">
    <StackLayout Margin="20">
        <Label Text="Denumire: " FontAttributes="Bold" />
        <Label Text="{Binding Denumire}" />
        <Button Text="Edit" Clicked="OnEditButtonClicked" />
        <Button Text="Delete" Clicked="OnDeleteButtonClicked" />
    </StackLayout>
</ContentPage>